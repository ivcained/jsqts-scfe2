  "packageManager": "yarn@3.2.3",
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  },
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^1.5.0",
    "@visx/curve": "^3.3.0",
